  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "login": {
        "required": [
          "username",
          "password"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "description": "input username",
            "example": "converter_user"
          },
          "password": {
            "type": "string",
            "description": "input password",
            "example": "12345weert"
          }
        }
      },
      "download": {
        "required": [
          "file_name"
        ],
        "type": "string",
        "example": "\"http://converter.com/download?file_name=file_to_download.zip\""
      },
      "videotoimages": {
        "required": [
          "fps",
          "input_file",
          "output_file",
          "checksum_param"
        ],
        "type": "object",
        "properties": {
          "input_file": {
            "type": "string",
            "description": "Choose the video to convert:",
            "format": "binary"
          },
          "output_file": {
            "type": "string",
            "description": "Choose the output format:",
            "example": "jpg",
            "enum": [
              "jpg",
              "png",
              "gif",
              "bmp",
              "tiff"
            ]
          },
          "fps": {
            "type": "string",
            "description": "Specify the number of images per second <i>(Recommended: 1)</i>:",
            "example": "1"
          },
          "checksum_param": {
            "type": "string",
            "description": "Specify the checksum param</i>:",
            "example": "12345abcdef"
          }
        }
      },
      "videotovideo": {
        "required": [
          "input_file",
          "output_file",
          "checksum_param"
        ],
        "type": "object",
        "properties": {
          "input_file": {
            "type": "string",
            "description": "Choose the video to convert:",
            "format": "binary",
            "example": "UG9sbGl0b3MubXA0"
          },
          "output_file": {
            "type": "string",
            "description": "Choose the output video format:",
            "default": "avi",
            "enum": [
              "avi",
              "mov",
              "ogg",
              "mpg"
            ]
          },
